CREATE VIEW bloomreader.v_avg_sessions_per_month_per_user AS
 WITH byuser AS (
         SELECT v_pages_read.anonymous_id,
            v_pages_read.book_branding,
            v_pages_read.country,
            date_trunc('month'::text, v_pages_read.time_local) AS month,
